// EXPECTED: 2
// EXPECTED: 5
// EXPECTED: 8
// EXPECTED: 9

FUNCTION bubbleSort(INT size)[STRING] {
    // Create array
    INT arr0 = 5
    INT arr1 = 2
    INT arr2 = 8
    INT arr3 = 1
    INT arr4 = 9
    
    // Bubble sort
    INT i = 0
    CYCLE WHILE i < size {
        INT maxJ = size - 1
        maxJ = maxJ - i
        INT j = 0
        CYCLE WHILE j < maxJ {
            INT current = 0
            INT next = 0
            
            IF j == 0 {
                current = arr0
                next = arr1
            }
            IF j == 1 {
                current = arr1
                next = arr2
            }
            IF j == 2 {
                current = arr2
                next = arr3
            }
            IF j == 3 {
                current = arr3
                next = arr4
            }
            
            IF current > next {
                INT temp = current
                IF j == 0 {
                    arr0 = arr1
                    arr1 = temp
                }
                IF j == 1 {
                    arr1 = arr2
                    arr2 = temp
                }
                IF j == 2 {
                    arr2 = arr3
                    arr3 = temp
                }
                IF j == 3 {
                    arr3 = arr4
                    arr4 = temp
                }
            }
            j = j + 1
        }
        i = i + 1
    }
    
    PRINT arr0
    PRINT arr1
    PRINT arr2
    PRINT arr3
    PRINT arr4
    RETURN "Done"
}

bubbleSort(5)

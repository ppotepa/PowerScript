// Fibonacci as proof of Turing completeness through recursion
FUNCTION fibRecursive(INT n)[INT] {
    IF n <= 1 {
        RETURN n
    }
    RETURN fibRecursive(n - 1) + fibRecursive(n - 2)
}

CYCLE 6 {
    PRINT fibRecursive(A)
}

// EXPECTED: olleH

// Test mixed custom syntax with strings
LINK "stdlib/syntax/strings.psx"

STRING greeting = "  Hello  "

// Test Trim then Contains (manual function calls to avoid :: bug)
FLEX trimmed = STR_TRIM(greeting)
FLEX contains = STR_CONTAINS(trimmed, "Hello")
PRINT contains

// Test StartsWith
FLEX starts = STR_STARTS_WITH(trimmed, "Hello")
PRINT starts

// Test Reverse
FLEX reversed = STR_REVERSE(trimmed)
PRINT reversed

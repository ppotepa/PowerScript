5

// Test chaining custom syntax operators
LINK "stdlib/syntax/arrays.psx"

FLEX numbers = [5, 2, 8, 1, 3]

// Test chaining: Sort() then First()
PRINT numbers::Sort()::First()

// Test chaining: Sort() then accessing by index
FLEX sorted = numbers::Sort()
PRINT sorted[1]

// Test chaining: Max()
PRINT numbers::Max()

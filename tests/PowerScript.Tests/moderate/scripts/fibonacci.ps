FUNCTION fibonacci(INT n)[INT] {
    IF n <= 1 {
        RETURN n
    }
    INT prev = 0
    INT curr = 1
    INT i = 2
    CYCLE WHILE i <= n {
        INT temp = prev + curr
        prev = curr
        curr = temp
        i = i + 1
    }
    RETURN curr
}

CYCLE 10 {
    PRINT fibonacci(A)
}

LINK "../../stdlib/IO.ps"
LINK System
FUNCTION isPrime(INT n)[INT] {
    IF n <= 1 {
        RETURN 0
    }
    IF n == 2 {
        RETURN 1
    }
    FLEX i = 2
    CYCLE n - 2 {
        IF n % i == 0 {
            RETURN 0
        }
        FLEX i = i + 1
    }
    RETURN 1
}

PRINT(isPrime(17))
PRINT(isPrime(18))

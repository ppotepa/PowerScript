// LINK "scripts/stdlib/ArrayLib.ps"

// Inlined function
FUNCTION ARRAY_CONTAINS(INT CHAIN arr, INT size, INT value)[INT] {
    CYCLE size AS i {
        FLEX elem = arr[i]
        IF elem == value {
            RETURN 1
        }
    }
    RETURN 0
}

// Test: ARRAY_CONTAINS - Value not found
FLEX arr = CHAIN OF [1, 2, 3, 4, 5]
FLEX result = ARRAY_CONTAINS(arr, 5, 10)
PRINT result
// Expected: 0

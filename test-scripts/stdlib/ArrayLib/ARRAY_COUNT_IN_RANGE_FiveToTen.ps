// LINK "scripts/stdlib/ArrayLib.ps"

FUNCTION ARRAY_COUNT_IN_RANGE(INT CHAIN arr, INT size, INT min, INT max)[INT] {
    FLEX count = 0
    CYCLE size AS i {
        FLEX elem = arr[i]
        IF elem >= min {
            IF elem <= max {
                count = count + 1
            }
        }
    }
    RETURN count
}

// Test: ARRAY_COUNT_IN_RANGE - Count in range
FLEX arr = CHAIN OF [5, 12, 8, 3, 15, 7]
FLEX result = ARRAY_COUNT_IN_RANGE(arr, 6, 5, 10)
PRINT result
// Expected: 3 (5, 8, 7)

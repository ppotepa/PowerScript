// LINK "scripts/stdlib/ArrayLib.ps"

// Inlined function
FUNCTION ARRAY_SUM(INT CHAIN arr, INT size)[INT] {
    FLEX sum = 0
    CYCLE size AS i {
        FLEX elem = arr[i]
        sum = sum + elem
    }
    RETURN sum
}

// Test: ARRAY_SUM - Sum all elements
FLEX arr = CHAIN OF [2, 4, 6, 8, 10]
FLEX result = ARRAY_SUM(arr, 5)
PRINT result
// Expected: 30

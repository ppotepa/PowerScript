// LINK "scripts/stdlib/ArrayLib.ps"

FUNCTION ARRAY_EQUALS(INT CHAIN arr1, INT size1, INT CHAIN arr2, INT size2)[INT] {
    IF size1 != size2 {
        RETURN 0
    }
    CYCLE size1 AS i {
        FLEX elem1 = arr1[i]
        FLEX elem2 = arr2[i]
        IF elem1 != elem2 {
            RETURN 0
        }
    }
    RETURN 1
}

// Test: ARRAY_EQUALS - Equal arrays
FLEX arr1 = CHAIN OF [1, 2, 3]
FLEX arr2 = CHAIN OF [1, 2, 3]
FLEX result = ARRAY_EQUALS(arr1, 3, arr2, 3)
PRINT result
// Expected: 1

LINK "../../stdlib/IO.ps"
LINK System

FLEX evenSum = 0
FLEX oddSum = 0
CYCLE 10 AS i {
    FLEX num = i + 1
    FLEX remainder = num / 2
    FLEX doubled = remainder * 2
    IF doubled == num {
        evenSum = evenSum + num
    } ELSE {
        oddSum = oddSum + num
    }
}
PRINT(evenSum)
PRINT(oddSum)
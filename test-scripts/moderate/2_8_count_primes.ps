LINK "../../stdlib/IO.ps"
LINK System

FLEX primes = 0
CYCLE 20 AS num_idx {
    FLEX num = num_idx + 2
    FLEX isPrime = 1
    IF num > 2 {
        CYCLE 10 AS div_idx {
            FLEX divisor = div_idx + 2
            IF divisor < num {
                FLEX quotient = num / divisor
                FLEX product = quotient * divisor
                IF product == num {
                    FLEX isPrime = 0
                }
            }
        }
    }
    IF isPrime == 1 {
        FLEX primes = primes + 1
    }
}
PRINT(primes)
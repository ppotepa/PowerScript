LINK System

FLEX dynamicValue = 42
PRINT "Dynamic FLEX variable initialized"

FUNCTION demonstrateFlex() {
    FLEX changing = 10
    PRINT "FLEX can change types freely"
}

FUNCTION demonstrateTyped(INT number)[INT] {
    INT result = number * 2
    RETURN result
}

FUNCTION demonstrateDotNet() {
    PRINT "Calling .NET directly:"
    NET::System.Console.WriteLine("Hello from .NET Framework!")
}

demonstrateFlex()
demonstrateTyped(50)
demonstrateDotNet()

PRINT "All features demonstrated successfully"

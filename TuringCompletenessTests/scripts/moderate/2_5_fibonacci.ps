FLEX n = 8
FLEX a = 0
FLEX b = 1
FLEX result = 0
IF n == 0 {
    FLEX result = 0
} ELSE {
    IF n == 1 {
        FLEX result = 1
    } ELSE {
        CYCLE 6 AS i {
            FLEX next = a + b
            FLEX a = b
            FLEX b = next
            FLEX result = next
        }
    }
}
PRINT result
